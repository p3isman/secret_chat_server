{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb", "airbnb-typescript/base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "consistent-return": 0,
    "linebreak-style": 0
  }
}
